<div>
  <h2>Edit The Article - <%= @article.title  %> </h2>
  <%= form_for(@article) do |f| %>
    <div>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div>
      <%= f.label :content %>
      <%= f.text_field :content %>
    </div>
    <%= f.submit %>
  <% end %>
</div>
